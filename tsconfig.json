{
	"compilerOptions": {
		"outDir": "./dist",
		"target": "esnext",
		"moduleResolution": "node",
		"module": "commonjs",
		"noImplicitAny": false,
		"removeComments": true,
		"sourceMap": true,
		"jsx": "react",
		"sourceRoot": "./src/",
		"jsxFactory": "h",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
	},
	"outDir": "./dist/",
	"include": [
		"src/**/*"
	],
	"suppressImplicitAnyIndexErrors": true,
